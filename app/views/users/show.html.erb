<div class="maincontent">
  <%= form_with(url: search_memos_path, local: true, method: :get, class: "search") do |form| %>
    <%= form.text_field :keyword, placeholder: " \uf002 seach", class: "fa" %>
    <%= form.submit "検索", class: "search-btn" %>
  <% end %>

<div class="contents">
    <h3><%= @nickname %>さんの投稿一覧</h3>

    <% @memos.each do |memo| %>
      <%= link_to memo.title, memo_path(memo.id), class: "contents__post" %>
  
    <% end %>
</div>
</div>  